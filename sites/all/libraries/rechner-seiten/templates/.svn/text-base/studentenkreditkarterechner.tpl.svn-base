


<%%beginform%%>
	 <script src="http://www.financeads.net/javascript/boxover.js" type="text/javascript"></script>
	 <form name="kontenrechner" action="" method="post" enctype="multipart/form-data" style="border:0px;margin:0px;padding:0px;display:inline;">
		 
          
          <table width="500" cellspacing="2" cellpadding="3">

            <tr><th colspan="2" class="fa_rahmen">Kostenrechner Studentenkreditkarte</th></tr>

            <tr class="fa_zeile">
              <td valign="top" width="50%">

                <table width="100%" cellspacing="0" cellpadding="3">
                  <tr>
                    <td width="20" valign="top"><img src="http://www.financeads.net/tools/img/nr_1.gif" alt="Schritt 1" /></td>
                    <td class="fa_zeile" valign="top">Jahresumsatz im Euroland:<br />

                      <small><i>nur Zahl z.B. 20000</i></small></td>
                    <td align="right" valign="top" class="fa_zeile">
                      <div style="white-space:nowrap;">{feld:umsatzeuroland} &euro;</div>
                    </td>
                  </tr>
                </table>

              </td>

              <td rowspan="2" valign="top" width="50%">

                <table width="100%" cellspacing="0" cellpadding="3">
                  <tr>
                    <td width="20" valign="top"><img src="http://www.financeads.net/tools/img/nr_3.gif" alt="Schritt 4" /></td>
                    <td colspan="2" class="fa_zeile">Nur bestimmte Karten anzeigen</td>                    
                  </tr>
				   <tr>
                    <td width="20" valign="top"></td>

                    <td class="fa_zeile">Kartengesellschaft</td>
                    <td align="right" valign="top" class="fa_zeile">
                     {feld:kartengesellschaft}
                    </td>
                  </tr>
				  <tr>
                    <td width="20" valign="top"></td>
                    <td class="fa_zeile">Ansehen / Status</td>
                    <td align="right" valign="top" class="fa_zeile">
                      {feld:anzeige}
                    </td>
                  </tr>
				  <tr>
                    <td width="20" valign="top"></td>
                    <td class="fa_zeile">Zahlungsart</td>
                    <td align="right" valign="top" class="fa_zeile">
                      {feld:zahlungsart}
                    </td>
                  </tr>
                </table>

              </td>
            </tr>

            <tr class="fa_zeile">
              <td valign="top" width="50%">

                <table width="100%" cellspacing="0" cellpadding="3">
                  <tr>
                    <td width="20" valign="top"><img src="http://www.financeads.net/tools/img/nr_2.gif" alt="Schritt 2" /></td>
                    <td class="fa_zeile" valign="top">Jahresumsatz im Nicht-Euroland:<br />
                      <small><i>nur Zahl z.B. 20000</i></small></td>

                    <td align="right" valign="top" class="fa_zeile">
                      <div style="white-space:nowrap;">{feld:umsatznichteuroland} &euro;</div>
                    </td>
                  </tr>
                </table>

              </td>
             
            </tr>
			
			<tr class="fa_zeile">

              <td colspan="2" align="right">
              
               
                <input type="submit" alt="Kreditkarten vergleichen" value="Beste Kreditkarten finden" class="fa_button" name="submit" />
				<input type="hidden" name="usersubmit" value="true" />
               
              </td>
            </tr>
          </table>
		</form>
<%%endform%%>

<%%begintable_start%%>

	<br />
	<table border="0" cellspacing="2" cellpadding="3" width="500">
	<tr>
		<th colspan="6" class="fa_rahmen">Kreditkarten nach Gesamtkosten*</th>
	</tr>
	<tr class="fa_zeile">
		<th></th>
		<th colspan="2">Anbieter / Kreditkarte</th>		
		<th>Geb&uuml;hren p.a. /<br />Extras</th>
		<th>Gesamt-<br />kosten p.a.</th>		
		<th>Mehr Infos /<br />Anfrage</th>	
	</tr>

	

<%%begintable_ende%%>

	<%%begindata%%>
	
		<tr class="fa_zeile">
			<td align="center" valign="middle">{nr}.</td>
			<td valign="middle" align="center" style="text-align:center;margin:0px;padding:0px;">
				<a href="{link}" rel="nofollow" target="_blank" onclick="blur()">
					<img src="{logo}" class="fa_logoimg" alt="" border="0" />
				</a>
			</td>
			<td>
				<a href="{link}" rel="nofollow" target="_blank" onclick="blur()" style="text-decoration:none;">
					<font class="fa_zeile fa_anbieter">
						{bank}
					</font>
					<br />
					<font class="fa_zeile fa_produkt">
						{produkt}
					</font>
				</a> 
			</td>
			<td align="center" valign="middle">	
				{gebuehren}	&euro;<br />
				{bonusprogram} {insurances} {benefits} {services}				
			</td>
			<td align="center" valign="middle">
				{kosten} &euro;	
			</td>
			<td align="center" valign="middle">
				<a href="{link}" rel="nofollow" target="_blank" onclick="blur()" class="fa_link">
				zum&nbsp;Anbieter
				</a>
			</td>
		</tr>
	
	<%%enddata%%>
	
	<%%begindata2%%>
	
		<tr class="fa_zeile2">
			<td align="center" valign="middle">{nr}.</td>
			<td valign="middle" align="center" style="text-align:center;margin:0px;padding:0px;">
				<a href="{link}" rel="nofollow" target="_blank" onclick="blur()">
					<img src="{logo}" class="fa_logoimg" alt="" border="0" />
				</a>
			</td>
			<td>
				<a href="{link}" rel="nofollow" target="_blank" onclick="blur()" style="text-decoration:none;">
					<font class="fa_zeile2 fa_anbieter">
						{bank} 
					</font>
					<br />
					<font class="fa_zeile2 fa_produkt">
						{produkt}
					</font>
				</a> 
			</td>
			<td align="center" valign="middle">	
				{gebuehren}	&euro;<br />
				{bonusprogram} {insurances} {benefits} {services}
			</td>
			<td align="center" valign="middle">
				{kosten} &euro;
			</td>
			<td align="center" valign="middle">
				<a href="{link}" rel="nofollow" target="_blank" onclick="blur()" class="fa_link">
				zum&nbsp;Anbieter
				</a>
			</td>
		</tr>
	
	<%%enddata2%%>
	
<%%endtable_start%%>
	<tr>
		<td colspan="6" class="fa_footnotes">
		* Werte f&uuml;r gew&auml;hlten Zeitraum. Alle Angaben ohne Gew&auml;hr
		</td>
	</tr>
	</table>

<%%endtable_ende%%>	